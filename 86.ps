/x 40 def
/Y 770 def
/colw 300 def

/dolandscape where { pop 612 0 translate 90 rotate /x 25 def /Y 590 def /colw 190 def } if

x Y moveto
R

/nextcol {
    /x x colw add def
    x Y moveto
} def

/sigil <5E> def
/printline {
    (#) anchorsearch { % suppress lines beginning with #
        pop pop 
    }{
        (:) search { pop pop } if  % suppress short code
        gsave
            sigil {
                search {
                    show exch token pop exec exch
                }{
                    show exit
                } ifelse
            } loop
        grestore
        0 lead neg rmoveto
    } ifelse
} def


